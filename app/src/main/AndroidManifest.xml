<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apk_kpa.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        android:debuggable="true"
        >
        <activity
            android:name="com.example.apk_kpa.app.MainActivity"
            android:label="@string/app_name"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.apk_kpa.app.Register"
            />
        <activity android:name="com.example.apk_kpa.app.Login"
            />
        <activity android:name=".LogedIn"
            />
        <activity android:name=".Admin"
            />
        <activity android:name="com.example.apk_kpa.app.Apklausa"
            />
        <activity android:name="com.example.apk_kpa.app.SwipeMenu.SwipeAdmin"
            />
        <activity android:name="com.example.apk_kpa.app.SwipeMenu.SwipeUser"
            />
        <activity android:name=".SukurtiKlausima"
            />



    </application>

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>
