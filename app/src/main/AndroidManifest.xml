<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.apk_kpa.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_apklausa"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.apk_kpa.app.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.apk_kpa.app.Register" />
        <activity android:name="com.example.apk_kpa.app.LogedIn" />
    </application>

    <uses-permission android:name="android.permission.INTERNET" />

</manifest>
